{
  "builds": [
    {
      "src": "vercel_app/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "handle": "filesystem",
      "src": "/(.*)",
      "filesystem": {
        "path": "/staticfiles"
      }
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app/wsgi.py"
    }
  ]
}

